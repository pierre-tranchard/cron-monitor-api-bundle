{
  "name": "pierre-tranchard/cron-monitor-api-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "The server side for exposing an API to monitor your cron via the cron-monitor-bundle",
  "autoload": {
    "psr-4": {
      "Tranchard\\CronMonitorApiBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tranchard\\CronMonitorApiBundle\\Tests\\": ""
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/symfony": "^3.4|^4.0@beta",
    "symfony/yaml": "^3.4|^4.0@beta",
    "alcaeus/mongo-php-adapter": "^1.1",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "symfony/serializer": "^3.4|^4.0@beta",
    "symfony/swiftmailer-bundle": "^3.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.0|^4.0@beta",
    "phpunit/phpunit": "*"
  }
}
